<table class="table table-bordered text-center">
  <thead>
    <tr>
      <th scope="col">Номер</th>
      <th scope="col">Текст вопроса</th>
    </tr>
  </thead>
    <tbody>
      <% @questions.each do |question| %>
          <td><%= question.id %></td>
          <td><%= question.body %></td>
        </tr>
      <% end %>
  </tbody>
</table>
