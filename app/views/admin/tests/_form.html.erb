<%= form_for [:admin, @test], local: true do |form| %>
 <%= render 'shared/errors', resource: @test %>

 <p>
   <%= form.label :title %><br>
   <%= form.text_field :title %>
 </p><br>

 <p>
   <%= form.label :level %><br>
   <%= form.number_field :level %>
 </p>

 <p>
  <%= form.label :timer %><br>
  <%= form.number_field :timer, in: 300.0..1800.0, step: 5 %>
</p>

 <p>
   <%= form.label :category_id %>:
   <%= form.collection_select :category_id, Category.all, :id, :title, prompt: true %>
 </p>

 <p><%= form.submit %></p>
<% end %>
